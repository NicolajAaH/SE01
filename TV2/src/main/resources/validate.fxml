    <children>
        <Button fx:id="signout" layoutX="923.0" layoutY="14.0" mnemonicParsing="false" onAction="#signoutHandler" text="Sign out" />
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backHandler" text="Back" />
      <ListView fx:id="validationsList" layoutX="60.0" layoutY="91.0" prefHeight="466.0" prefWidth="764.0" />
      <Label layoutX="60.0" layoutY="64.0" text="Needs validation:" />
      <Button fx:id="validateButton" layoutX="875.0" layoutY="91.0" mnemonicParsing="false" text="Validate" onAction="#validateHandler"/>

    </children>
</AnchorPane>
